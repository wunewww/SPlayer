    "axios": "^1.6.0",
    "colorthief": "^2.4.0",
    "electron-dl": "^3.5.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "howler": "^2.2.3",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "music-metadata": "7.13.4",
    "node-id3": "^0.2.6",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "plyr": "^3.7.8",
