    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "ajv": "^8.12.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.9.1",
    "electron-log": "^5.0.1",
    "electron-vite": "^1.0.29",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "naive-ui": "^2.36.0",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "terser": "^5.19.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.12",
